<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="rmqMessagingGateway" type="paramore.brighter.commandprocessor.messaginggateway.rmq.MessagingGatewayConfiguration.RMQMessagingGatewayConfigurationSection, paramore.brighter.commandprocessor.messaginggateway.rmq" />
  </configSections>
  <appSettings>
    <add key="etcd" value="http://192.168.99.100:32770"/>
    <add key="env" value="development"/>
    <add key="module" value="access-control"/>
  </appSettings>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
    </startup>
  <rmqMessagingGateway>
    <amqpUri uri="amqp://guest:guest@192.168.99.100:32780/%2f" connectionRetryCount="3" retryWaitInMilliseconds="3000" circuitBreakTimeInMilliseconds="60000" />
    <exchange name="paramore.brighter.exchange" type="direct" durable="true" />
    <queues highAvailability="false" qosPrefetchSize="1" />
  </rmqMessagingGateway>
</configuration>
