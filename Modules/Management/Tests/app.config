<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog"/>
    <section name="rmqMessagingGateway" type="paramore.brighter.commandprocessor.messaginggateway.rmq.MessagingGatewayConfiguration.RMQMessagingGatewayConfigurationSection, paramore.brighter.commandprocessor.messaginggateway.rmq"/>
  </configSections>
  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" throwExceptions="true" internalLogFile="C:\logfile.txt" internalLogLevel="Debug">
    <extensions>
      <add assembly="NLog.Targets.Syslog"/>
    </extensions>
    <targets>
      <target name="syslog" type="Syslog" syslogserver="logs2.papertrailapp.com" port="34308" facility="Local7"/>
    </targets>
    <rules>
      <logger name="paramore.*" minlevel="Debug" writeTo="syslog"/>
      <logger name="Trackwane.*" minlevel="Debug" writeTo="syslog"/>
    </rules>
  </nlog>
  <appSettings>
    <add key="document-store:use-embedded" value="true"/>
    <add key="platform:secretKey" value="GQDstcKsx0NHjPOuXOYg5MbeJ1XT0uFiwDVvVBrk"/>
  </appSettings>
  <rmqMessagingGateway>
    <amqpUri uri="amqp://guest:guest@localhost:5672/%2f" connectionRetryCount="3" retryWaitInMilliseconds="3000" circuitBreakTimeInMilliseconds="60000"/>
    <exchange name="paramore.brighter.exchange" type="direct" durable="true"/>
    <queues highAvailability="false" qosPrefetchSize="1"/>
  </rmqMessagingGateway>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/></startup></configuration>
